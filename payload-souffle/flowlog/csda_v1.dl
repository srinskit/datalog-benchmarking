.in
.decl NullEdge(x: number, y: number)
.input NullEdge.csv

.decl Edge(x: number, y: number)
.input Edge.csv

.printsize 
.decl NullNode(x: number, y: number)

.rule
NullNode(x, y) :- NullEdge(x, y).
# Reordered: moved Edge(w, y) before NullNode(x, w) to change join order
NullNode(x, y) :- Edge(w, y), NullNode(x, w).
