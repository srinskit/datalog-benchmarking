EDB_DECL:
Arc(x int, y int, l int)

IDB_DECL:
Zero(x int, y int)
One(x int, y int)
Dyck(x int, y int)

RULE_DECL:
Zero(x, y) :- Arc(x, y, 0).
One(x, y) :- Arc(x, y, 1).

Dyck(x, y) :- Zero(x, z), Zero(z, y).
Dyck(x, y) :- One(x, z), One(z, y).
Dyck(x, y) :- Dyck(z, w), Zero(w, y), Zero(x, z).
Dyck(x, y) :- One(x, z), Dyck(z, w), One(w, y).
Dyck(x, y) :- Dyck(x, z), Dyck(z, y).